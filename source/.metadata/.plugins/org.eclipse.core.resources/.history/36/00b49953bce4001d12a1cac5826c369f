<mapper namespace="kr.or.forcewave.index.dao.IndexDAO">
	
	<select id="selectXY" parameterType="String" resultType="hashMap">
		SELECT
			ADM_X x
			, ADM_Y y
		FROM MAPOGU_EMD
		WHERE ADM_NM = #{admNm}
	</select>
	
</mapper>