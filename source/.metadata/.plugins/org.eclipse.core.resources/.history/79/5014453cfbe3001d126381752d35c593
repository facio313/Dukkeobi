	@Override
	public List<SafetyVO> retrieveSafetyList() {
		// 유효성 검사..
		return dao.selectSafetyList();
	}

	@Override
	public SafetyVO retrieveSafety() {
		// TODO Auto-generated method stub
		return null;
	}

}
